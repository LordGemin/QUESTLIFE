<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="500.0" prefWidth="350.0" styleClass="background" stylesheets="@DarkTheme.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.com.questlife.questlife.View.TempleDialogController">
   <children>
      <TableView fx:id="rewardTableView" layoutX="14.0" layoutY="14.0" prefHeight="430.0" prefWidth="350.0" AnchorPane.bottomAnchor="70.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columns>
          <TableColumn fx:id="rewardName" prefWidth="75.0" text="Name" />
          <TableColumn fx:id="rewardType" prefWidth="75.0" text="Type" />
            <TableColumn fx:id="rewardAssociatedSkill" prefWidth="75.0" text="Skill" />
            <TableColumn fx:id="rewardCost" prefWidth="75.0" text="Cost" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <ButtonBar layoutX="21.0" layoutY="430.0" prefHeight="40.0" prefWidth="293.0" AnchorPane.bottomAnchor="30.0" AnchorPane.rightAnchor="25.0">
        <buttons>
          <Button mnemonicParsing="false" onAction="#handleAcquisition" text="Buy..." />
            <Button mnemonicParsing="false" onAction="#handleEdit" text="Edit..." />
            <Button mnemonicParsing="false" onAction="#handleCancel" text="Leave" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
